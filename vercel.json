{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["index.html", "loader.gif"]
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/loader.gif",
      "dest": "/loader.gif"
    },
    {
      "src": "/chat",
      "dest": "/chat"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "API_KEY": "AIzaSyCCGVF7WixN7u7A5IrZ7hwaUQ1KDO2k8Yo"
  }
}
